# Argument for set_g and map_g for the case of symbols.


package symbol_ord_key {

    Key = symbol::Symbol;

    fun compare (s1, s2)
        =
	if     (symbol::symbol_fast_lt (s1, s2))   LESS;
	elif   (symbol::eq (s1, s2)            )   EQUAL;
	else                                       GREATER;
        fi;
};


