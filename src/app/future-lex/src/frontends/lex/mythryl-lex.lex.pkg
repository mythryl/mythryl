	fun mk_stream read_n
            =
	    STREAM { stream, id => next_id(), pos => init_pos, line_no => 1 }
            where 

                stream = tsio::make_instream 
			   (tpio::STREAM_READER {
			        name => "lexgen",
				chunk_size => 4096,
				read_vector => THE read_n,
