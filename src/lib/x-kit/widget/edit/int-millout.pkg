herein

    package int_millout								# 
    {
	Int_Millout
	  =	
	  { note_watcher:	(mt::Inport, (Id, Int) -> Void) -> Void, 	# The 'Id' is server's, identifiers the particular server for watcher.
	    drop_watcher:	 mt::Inport -> Void				# The mt::Inport must match that given to note_watcher (or texteditor_imp::WATCHER).
	  };				 

	exception  INT_MILLOUT  Int_Millout;					# We'll never 'raise' this exception:  It is purely a datastructure to hide the Int_Millout type from millboss-imp.pkg, in the interests of good modularity.
	#
