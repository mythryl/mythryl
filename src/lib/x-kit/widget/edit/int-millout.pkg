			    raise exception DIE msg;						# Just to keep compiler happy.
			};
	    esac;	


	fun wrap__int_millout
	      (
		outport:	mt::Outport,
		int_millout:	Int_Millout
	      ):		mt::Millout
	    =
	    { outport,
	      port_type =>  "int_millout::Int_Millout",
	      data_type =>  "Int",
	      info	=>  "Wrapped by int_millout::wrap__int_millout.",
	      crypt	=>  INT_MILLOUT int_millout,
	      watchers	=>  mt::ips::empty	
	    };		 
    };

