package main:  api my main:  String * List( String ) -> winix__premicrothresd::process::status end  {

  package m = ml_code_monkey

  fun main0 (prog, files) = apply m::gen files

  fun main args = (main0 args; winix__premicrothresd::process::success)
      except exn => (file::write (file::stderr, exceptions::exception_message exn);
	             file::write (file::stderr, "\n");
		     winix__premicrothresd::process::failure)
};
