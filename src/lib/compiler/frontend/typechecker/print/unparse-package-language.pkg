	also
	fun unparse_closure stream (m::GENERIC_CLOSURE {   parameter_module_stamp    => parameter,
                                                             body_package_expression => body,
                                                             typechecker_dictionary       => symbol_table
                                                         },
                                                         depth
                                      )
	    =
