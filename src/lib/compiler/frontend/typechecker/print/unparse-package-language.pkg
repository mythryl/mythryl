		    unparse_closure stream (generic_closure, depth - 1);
		    newline();
		    pps "lambdaty:";
		    break { spaces=>1, indent_on_wrap=>2 };
		    unparse_lty stream ( /* ModulePropLists::genericMacroExpansionLty e, depth - 1 */ );
