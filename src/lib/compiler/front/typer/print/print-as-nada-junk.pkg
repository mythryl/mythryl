    fun print_sequence0_as_nada ppstream (sep: pp::Stream -> Void, pr, elems)
	=
	{ fun pr_elems [el] => pr ppstream el;
	       pr_elems (el ! rest) =>
		  { pr ppstream el;
		   sep ppstream;
		   pr_elems rest;};
	       pr_elems [] => (); end;
	
	    pr_elems elems;
	};

     Break_Style
        =
