        =
	{   pp::begin_horizontal_else_vertical_box ppstream;
	    front ppstream;
	    open_style_box style ppstream (pp::CURSOR_RELATIVE 0);
	    print_sequence0_as_nada ppstream (sep, pr, elems); 
	    pp::end_box ppstream;
	    back ppstream;
	    pp::end_box ppstream
        ;};

