			    pp::string stream " =";

			    break stream { spaces=>1, indent_on_wrap=>0 };
                            unparse_type context stream (definition, d);
			    end_box stream;
			};

		    unparse_named_type' (SOURCE_CODE_REGION_FOR_NAMED_TYPE (t, r), d)
