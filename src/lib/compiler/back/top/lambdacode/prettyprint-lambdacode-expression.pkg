                            };
			    pp.exd " ";
			    pp.lit ")";
                        };

		    do (lcf::GENOP (dictionary, p, t, ts))
                        => 
			pp.box' 0 0 .{
			    pp.lit "lcf::GENOP (";
			    pp.ind " ";
			    pp.txt " ";

			    pp.lit (hbo::baseop_to_string p);

			    pp.endlit ",";
			    pp.txt " ";	

			    hcf::prettyprint_uniqtypoid  pp  t;

			    pp.endlit ",";
			    pp.txt " ";	

			    pp.box' 0 0 .{
				pp.lit "[";
				pp.ind " ";
				pp.txt " ";

				pp::seq  .{ pp.txt ", "; }   (hcf::prettyprint_uniqtype pp)  ts;

