	fun print_lexp (pp:Pp) l
	    = 
	    do l
	    where	
	        fun pr_lty t =  pp.lit (hcf::uniqtypoid_to_string t);
		fun pr_typ t =  pp.lit (hcf::uniqtype_to_string t);
		fun pr_knd k =  pp.lit (hcf::uniqkind_to_string k);

		fun plist (p, [],    sep) =>    ();
		    #
		    plist (p, a ! r, sep) =>    {   p a;
						    #
						    apply f r
						    where
							fun f x
							    =
							    {   pp.lit sep;
								p x;
							    };
						    end;
						};
                end;

		fun do (lcf::VAR     v) =>  pp.lit (name_of_highcode_codetemp v);
		    do (lcf::INT     i) =>                    pp.lit (int::to_string   i);
		    do (lcf::UNT     i) =>  { pp.lit "(U)";   pp.lit (unt::to_string   i); };
		    do (lcf::INT1    i) =>  { pp.lit "(I32)"; pp.lit (one_word_int::to_string i); };
		    do (lcf::UNT1    i) =>  { pp.lit "(U32)"; pp.lit (one_word_unt::to_string i); };
		    do (lcf::FLOAT64 s) =>  pp.lit s;
		    do (lcf::STRING  s) =>  pp.lit (heap_string s);
		    do (lcf::EXCEPTION_TAG (l, _)) => do l;

		    do (r as lcf::RECORD l)
			=>
			pp.box' 0 0 .{
			    pp.lit "lcf::RECORD {";
			    pp.ind " ";
			    pp.txt " ";
			    pp::seq .{ pp.txt ", "; }  do l;
			    pp.exd " ";
			    pp.lit "}";
			};

		    do (r as lcf::PACKAGE_RECORD l)
			=>
			pp.box' 0 0 .{
			    pp.lit "lcf::PACKAGE_RECORD {";
			    pp.ind " ";
			    pp.txt " ";
			    pp::seq .{ pp.txt ", "; }  do l;
			    pp.exd " ";
			    pp.lit "}";
			};

		    do (r as lcf::VECTOR (l, _))
			=>
			pp.box' 0 0 .{
			    pp.lit "lcf::VECTOR [";
			    pp.ind " ";
			    pp.txt " ";
			    pp::seq .{ pp.txt ", "; }  do l;
			    pp.exd " ";
			    pp.lit "]";
			};

		    do (lcf::BASEOP (p, t, ts))
			=> 
			pp.box' 0 0 .{
			    pp.lit "lcf::BASEOP (";
                            pp.ind " ";
			    pp.txt " ";
			    pp.lit (hbo::baseop_to_string p);
			    pp.txt ", ";
                            hcf::prettyprint_uniqtypoid  pp  t; 
			    pp.endlit ",";
			    pp.txt " ";
			    pp.box' 0 0 .{
				pp.lit "[";
				pp.ind " ";
				pp.txt " ";
				pp::seq .{ pp.txt ", "; }  (hcf::prettyprint_uniqtype pp)  ts;
				pp.exd " ";
				pp.lit "]";
			    };	
                            pp.exd " ";
                            pp.lit ")";
                        };

		    do (l as lcf::GET_FIELD (i, _))
                        =>
		        {   fun gather (lcf::GET_FIELD (i, l))
                                    =>
				    {   (gather l) ->   (more, root);
					#
				        (i ! more,  root);
				    };

			        gather l =>   (NIL, l);
                            end;

			    (gather l) ->   (path, root);

			    fun ipr (i: Int)
                                =
                                pp.lit (int::to_string i);

			    do root;

			    pp.box' 0 0 .{
				pp.lit "lcf::BASEOP (";
				pp.ind " ";
				pp.txt " ";
				pp::seq  .{ pp.txt ", "; }  ipr  (reverse path);
				pp.exd " ";
				pp.lit ")";
			    };
		       };

		    do (lcf::FN (v, t, l))
                        => 
		        pp.box' 0 0 .{
			    pp.lit "lcf::FN (";
			    pp.ind " ";
			    pp.txt " ";

                            pp.lit (name_of_highcode_codetemp v);

			    pp.box' 0 0 .{
				pp.lit ":"; 
				pp.ind " "; 
				pp.txt " "; 

				hcf::prettyprint_uniqtypoid  pp  t;
			    };

                            pp.endlit ",";
			    pp.txt " ";

			    do l;

			    pp.exd " ";
			    pp.lit ")";
                        };

		    do (lcf::CONSTRUCTOR((s, c, lt), ts, l))
                        => 
		        pp.box' 0 0 .{
			    pp.lit "lcf::CONSTRUCTOR (";
			    pp.ind " ";
			    pp.txt " ";

			    pp.box' 0 0 .{
				pp.lit "(";
				pp.ind " ";
				pp.txt " ";

				pp.lit (sy::name s);
				pp.txt ", ";

				pp.lit (vh::print_representation c);
				pp.endlit ",";
				pp.txt " ";

				hcf::prettyprint_uniqtypoid  pp  lt;

				pp.exd " ";
				pp.lit ")";
			    };
			    pp.endlit ",";
			    pp.txt " ";

			    pp.box' 0 0 .{
				pp.lit "[";
				pp.ind " ";
				pp.txt " ";

				pp::seq .{ pp.txt ", "; }  (hcf::prettyprint_uniqtype pp)  ts;

				pp.exd " ";
				pp.lit "]";
			    };
			    pp.endlit ",";
			    pp.txt " ";

			    do l;

			    pp.exd " ";
