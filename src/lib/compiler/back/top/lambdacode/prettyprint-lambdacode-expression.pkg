			    pp.txt " ";
			    hcf::prettyprint_uniqtypoid  pp  lt;
                            pp.endlit ",";
			    pp.txt " ";

			    do l;

			    pp.exd " ";
			    pp.lit ")";
			};

		    do (lcf::SWITCH (l, _, llist, default))
