			    pp.txt " ";

			    app2 (  fn (tc, ntc)
					=
					{   pp.box' 0 0 .{
						pp.lit "<";
						hcf::prettyprint_uniqtype pp tc;
						pp.txt ", ";
						hcf::prettyprint_uniqtype pp ntc;
