		    do (lcf::TYPEFUN (ks, b))
                        => 
		        pp.box' 0 0 .{
			    pp.lit "lcf::TYPEFUN(";
			    pp.ind " ";
			    pp.txt " ";

                            pp::seq .{ pp.txt ", "; }  (hcf::prettyprint_uniqkind pp)  ks;

