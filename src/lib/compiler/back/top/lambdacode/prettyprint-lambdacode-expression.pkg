                        =>
                        pp.box' 0 0 .{
			    pp.lit "(lcf::APPLY(lcf::FN...))";
			    pp.txt " ";
                            do (lcf::LET (v, r, l));
                        };

		    do (lcf::LET (v, r, l))
                        => 
		        pp.box' 0 0 .{
			    pp.lit "lcf::LET";
			    pp.ind " ";
			    pp.txt " ";
			    lv = name_of_highcode_codetemp v;
			    len = size lv + 3;

			    pp.box' 0 0 .{	
				pp.lit lv;
				pp.lit " =";
				pp.ind " ";
				pp.txt " ";

				do r;
			    };

			    pp.exd " ";
			    pp.txt " IN ";	

                            do l;

			    pp.exd " ";
			    pp.lit "END";	
			};

		    do (lcf::APPLY (l, r))
                        => 
		        pp.box' 0 0 .{
			    pp.lit "lcf::APPLY(";
