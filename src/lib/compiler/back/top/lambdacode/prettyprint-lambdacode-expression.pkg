			    pp.endlit ",";
			    pp.txt    " ";
			    pp.box' 0 0 .{
                                pp.lit "[";
				pp.ind " ";
				pp.txt " ";

                                pp::seq .{ pp.txt ", "; }   (hcf::prettyprint_uniqtype pp)  ts;

				pp.exd " ";
