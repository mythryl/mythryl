local
  fun repeat c n = number_string::padLeft c n ""
  fun simple1 (name, w, n, open_box) () =
	withPP (name, w) (fn stream => (
	  open_box stream (pp::Rel 0);
	    pp::string stream (repeat 'x' n);
	    pp::cut stream;
	    pp::string stream (repeat 'y' n);
	    pp::cut stream;
	    pp::string stream (repeat 'z' n);
	  pp::end_box stream))
