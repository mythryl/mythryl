## winix-types.pkg


# This is the OS package (s) with only types, for compiling APIs.



package os {

	package W32G = win32_general

	type System_Error = W32G::word
	    
	package process {

		type status = W32G::word
	    }

	package io {

		enum Io_Descriptor = IODESC of Ref( win32_general::hndl )
		                | SockDesc of Int
	    }
};

package PreOS = os;



